{
  "name": "reactor",
  "description": "React + Electron + Nx starter project in typescript",
  "author": {
    "email": "blazeingamer10@gmail.com",
    "name": "BLaZeKiLL"
  },
  "keywords": [
    "React",
    "Electron",
    "nx",
    "nrwl",
    "typescript",
    "starter project"
  ],
  "version": "0.0.0",
  "license": "MIT",
  "main": "dist/apps/electron/main.js",
  "scripts": {
    "postinstall": "npx electron-builder install-app-deps",
    "nx": "nx",
    "start": "npm-run-all -p web:start:dev electron:start:dev ",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "web:start:dev": "nx serve react",
    "electron:ext-install": "yarn run electron:start --ext-install",
    "electron:start": "yarn run build:electron && npx electron .",
    "electron:start:dev": "npx wait-on http-get://localhost:4200/ && yarn run electron:start --serve",
    "electron:start:prod": "yarn run build:prod && npx electron .",
    "build:dev": "yarn run build:electron && yarn run build:web && node tools/builders/postbuild.js",
    "build:prod": "yarn run build:electron --prod && yarn run build:web --prod && node tools/builders/postbuild.js",
    "build:electron": "nx build electron",
    "build:web": "nx build react --base-href ./",
    "test:web": "nx test browser",
    "test:electron": "nx test electron",
    "package:windows": "npx electron-builder build --windows",
    "package:linux": "npx electron-builder build --linux",
    "package:mac": "npx electron-builder build --mac",
    "electron:windows": "yarn run build:prod && yarn run package:windows",
    "electron:linux": "yarn run build:prod && yarn run package:linux",
    "electron:mac": "yarn run build:prod && yarn run package:mac"
  },
  "private": false,
  "dependencies": {
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@babel/preset-react": "7.7.4",
    "@nrwl/cypress": "8.9.0",
    "@nrwl/eslint-plugin-nx": "8.9.0",
    "@nrwl/jest": "8.9.0",
    "@nrwl/node": "^8.6.0",
    "@nrwl/react": "^8.6.0",
    "@nrwl/web": "8.9.0",
    "@nrwl/workspace": "8.9.0",
    "@testing-library/react": "9.4.0",
    "@types/jest": "24.0.24",
    "@types/node": "~12.12.21",
    "@types/react": "16.9.16",
    "@types/react-dom": "16.9.4",
    "@types/react-router-dom": "5.1.3",
    "@typescript-eslint/eslint-plugin": "2.12.0",
    "@typescript-eslint/parser": "2.12.0",
    "cypress": "3.8.0",
    "devtron": "^1.4.0",
    "document-register-element": "1.14.3",
    "dotenv": "8.2.0",
    "electron": "^7.1.6",
    "electron-builder": "21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-reload": "^1.5.0",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "jest": "24.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "1.19.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-router-dom": "5.1.2",
    "rollup": "^1.25.1",
    "ts-jest": "24.2.0",
    "ts-node": "~8.5.4",
    "tslint": "~5.20.1",
    "typescript": "~3.7.3",
    "wait-on": "^3.3.0",
    "webpack": "^4.41.2",
    "webpack-sources": "^1.4.3"
  }
}
